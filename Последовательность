using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSpril
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int step = 7;

            int maximumNumber = 103;

            int number = 5;

            for (int i = number; i <= maximumNumber; i += step)
            {
                Console.WriteLine(i);
            }

            Console.ReadKey();
        }
    }
}
